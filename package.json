{
	"$schema": "https://json.schemastore.org/package",
	"name": "zod-fixture",
	"version": "0.0.0",
	"description": "Creating fixtures based on zod schemas",
	"packageManager": "npm@9.5.0",
	"scripts": {
		"start": "vite -c vite.lib.config.ts",
		"test": "vitest run -c vite.lib.config.ts",
		"test:watch": "vitest -c vite.lib.config.ts",
		"test:ui": "vitest -c vite.lib.config.ts --ui",
		"test:coverage": "vitest run --coverage -c vite.lib.config.ts",
		"build": "vite build -c vite.lib.config.ts",
		"lint": "eslint . --max-warnings 0",
		"prepare": "git config core.hookspath .githooks",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs",
		"readme:generate": "node ./scripts/readme",
		"readme:check": "git diff --name-only --exit-code README.md"
	},
	"files": [
		"dist"
	],
	"main": "./dist/zod-fixture.umd.cjs",
	"module": "./dist/zod-fixture.es.js",
	"exports": {
		".": {
			"import": "./dist/zod-fixture.es.js",
			"require": "./dist/zod-fixture.umd.cjs",
			"types": "./dist/public.d.ts"
		}
	},
	"types": "./dist/public.d.ts",
	"type": "module",
	"keywords": [
		"zod",
		"test",
		"data",
		"generator",
		"random",
		"fake",
		"fixtures",
		"contextual",
		"stub",
		"mock",
		"schema",
		"dummy"
	],
	"author": "Tim Deschryver",
	"license": "MIT",
	"peerDependencies": {
		"zod": ">=3.0.0"
	},
	"dependencies": {
		"randexp": "^0.5.3"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"@vitest/coverage-c8": "^0.30.1",
		"@vitest/ui": "^0.31.4",
		"c8": "^7.14.0",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.8.0",
		"lint-staged": "^13.2.3",
		"minimist": "^1.2.8",
		"prettier": "^3.0.0",
		"typescript": "^5.1.6",
		"vite": "^4.4.4",
		"vite-plugin-dts": "^1.7.3",
		"vitepress": "^1.0.0-beta.3",
		"vitest": "^0.31.4",
		"zod": "^3.21.4"
	},
	"imports": {
		"@/*": "./src/*",
		"zod-fixture": [
			"./src/public.ts"
		]
	}
}
