{
	"name": "zod-fixture",
	"version": "0.0.0",
	"description": "Creating fixtures based on zod schemas",
	"scripts": {
		"test": "vitest",
		"test:coverage": "vitest run --coverage",
		"build": "vite build",
		"lint": "eslint . --max-warnings 0",
		"pre-commit": "npm run lint && npm run test:coverage",
		"prepare": "git config core.hookspath .githooks"
	},
	"files": [
		"dist"
	],
	"main": "./dist/zod-fixture.umd.cjs",
	"module": "./dist/zod-fixture.es.js",
	"exports": {
		".": {
			"import": "./dist/zod-fixture.es.js",
			"require": "./dist/zod-fixture.umd.cjs"
		}
	},
	"types": "./dist/index.d.ts",
	"type": "module",
	"keywords": [
		"zod",
		"fixtures"
	],
	"author": "Tim Deschryver",
	"license": "MIT",
	"peerDependencies": {
		"zod": ">=3.0.0"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"@vitest/coverage-c8": "^0.30.1",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"prettier": "^2.8.8",
		"typescript": "^5.0.4",
		"vite": "^4.3.3",
		"vite-plugin-dts": "^2.3.0",
		"vitest": "^0.30.1",
		"zod": "^3.21.4"
	},
	"dependencies": {
		"@paralleldrive/cuid2": "^2.2.0",
		"cuid": "^3.0.0"
	}
}
